{
  "name": "ornamentum",
  "version": "1.1.32",
  "description": "Angular 2+ Data Table (Dev Inprogress)",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "test:ci": "ng test --single-run",
    "e2e": "ng e2e",
    "docs": "rimraf docs/api-docs && compodoc -p tsconfig.doc.json -d docs/api-docs --disableCoverage --disablePrivate --disableInternal --disableProtected",
    "lint:src": "ng lint",
    "lint:scss": "sass-lint --config .scss-lint.yml --no-exit --verbose",
    "lint": "npm run lint:src && npm run lint:scss",
    "start": "ng serve --open --port 4201",
    "build:dev": "ng build --progress --stats-json",
    "build:demo": "ng build --extract-css --progress --prod --aot --output-path docs --base-href /ornamentum/",
    "build:docs": "npm run build:demo && npm run docs",
    "clean:build": "rimraf coverage dist lib .tmp docs",
    "ng:compile:es5": "node_modules/.bin/ngc -p tsconfig.lib.es5.json",
    "ng:compile:es2015": "node_modules/.bin/ngc -p tsconfig.lib.es2015.json",
    "ng:compile:es": "gulp inline-templates && npm run ng:compile:es5 && npm run ng:compile:es2015",
    "build:copy:essentials": "gulp copy-package-essentials",
    "build:bundle": "gulp rollup-bundle",
    "build:lib": "npm run clean:build && npm run ng:compile:es && npm run build:copy:essentials && npm run build:bundle",
    "build": "npm run build:lib && npm run build:docs",
    "explorer:bundle": "npx webpack-bundle-analyzer dist/stats.json",
    "explorer": "npm run build:dev && npm run explorer:bundle",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags && ngh --dir docs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yohangz/ornamentum.git"
  },
  "keywords": [
    "Angular 2+",
    "grid"
  ],
  "contributors": [
    {
      "name": "Yohan Gomez",
      "email": "yohan.gz@gmail.com",
      "url": "https://github.com/yohangz"
    },
    {
      "name": "Lahiru Jayamanna",
      "email": "lsampath999@gmail.com",
      "url": "https://github.com/lahiruz"
    },
    {
      "name": "Samuditha Wadigamangawa",
      "email": "samuditha@live.com",
      "url": "https://github.com/samudithaw"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/yohangz/ornamentum/issues"
  },
  "homepage": "https://github.com/yohangz/ornamentum#readme",
  "dependencies": {
    "@angular/common": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "lodash": "^4.17.5",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cli": "~1.7.2",
    "@angular/compiler": "^5.2.7",
    "@angular/compiler-cli": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/language-service": "^5.2.7",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "~2.8.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.104",
    "@types/node": "~8.0.57",
    "@types/webpack": "^3.8.1",
    "angular-cli-ghpages": "^0.5.2",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^8.3.0",
    "awesome-typescript-loader": "^3.4.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "codelyzer": "^4.0.1",
    "compass-mixins": "^0.12.10",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "gulp": "^3.9.1",
    "gulp-file": "^0.4.0",
    "gulp-inline-ng2-template": "^4.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-loader": "^0.5.7",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
