{
  "name": "ornamentum-demo",
  "version": "2.0.0",
  "description": "Angular Toolkit",
  "scripts": {
    "ng": "ng",
    "test:demo": "ng test",
    "test:demo:ci": "ng test --single-run",
    "e2e:demo": "ng e2e",
    "lint:demo": "ng lint",
    "start:demo": "ng serve --open --port 4201",
    "build:demo:dev": "ng build --stats-json",
    "build:demo:prod": "ng build --prod",
    "lint:demo:scss": "sass-lint --config src/.scss-lint.yml --no-exit --verbose",
    "explorer:demo:bundle": "npx webpack-bundle-analyzer dist/ornamentum-demo/stats.json",
    "test:lib": "ng test ornamentum",
    "test:lib:ci": "ng test ornamentum --single-run",
    "e2e:lib": "ng e2e ornamentum",
    "lint:lib": "ng lint ornamentum",
    "build:lib:dev": "ng build ornamentum",
    "build:lib:prod": "ng build ornamentum --prod",
    "lint:lib:scss": "sass-lint --config projects/ornamentum/.scss-lint.yml --no-exit --verbose",
    "docs:lib": "compodoc -p projects/ornamentum/tsconfig.doc.json -d dist/ornamentum-demo/api-docs --disableCoverage --disablePrivate --disableInternal --disableProtected",
    "clean:dist": "rimraf dist",
    "lint": "npm run lint:demo && npm run lint:lib && npm run lint:demo:scss && npm run lint:lib:scss",
    "start": "npm run clean:dist && npm run build:lib:dev && ng serve --open --port 4201",
    "build": "npm run clean:dist && npm run build:lib:prod && npm run build:demo:prod && npm run docs:lib",
    "explorer": "npm run clean:dist && npm run build:lib:dev && npm run build:demo:dev && npm run explorer:demo:bundle",
    "docs:publish": "ngh --dir dist/ornamentum-demo",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags && npm run docs:publish",
    "prerelease": "npm run build",
    "release": "npm publish dist/ornamentum"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yohangz/ornamentum.git"
  },
  "keywords": [
    "Angular",
    "Grid",
    "Data Table",
    "Dropdown List",
    "Multi Select Dropdown",
    "Angular 6+",
    "UI Grid",
    "NG Grid",
    "NGX Grid"
  ],
  "contributors": [
    {
      "name": "Yohan Gomez",
      "email": "yohan.gz@gmail.com",
      "url": "https://github.com/yohangz"
    },
    {
      "name": "Lahiru Jayamanna",
      "email": "lsampath999@gmail.com",
      "url": "https://github.com/lahiruz"
    },
    {
      "name": "Samuditha Wadigamangawa",
      "email": "samuditha@live.com",
      "url": "https://github.com/samudithaw"
    }
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/yohangz/ornamentum/issues"
  },
  "homepage": "https://github.com/yohangz/ornamentum#readme",
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/animations": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26",
    "lodash-es": "^4.17.10",
    "bootstrap": "^4.1.1",
    "compass-mixins": "^0.12.10"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.0",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular-devkit/build-angular": "~0.6.1",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.1",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "@compodoc/compodoc": "^1.1.3",
    "@types/lodash-es": "^4.17.0",
    "angular-cli-ghpages": "^0.5.2",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "webpack-bundle-analyzer": "^2.10.1"
  }
}
